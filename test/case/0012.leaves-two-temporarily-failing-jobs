#!/bin/sh
set -ex
mkdir -p q/tmp q/queue q/done q/fail
touch q/tmp/001 q/tmp/002
mv q/tmp/001 q/tmp/002 q/queue
fsq-run ./q sh -c 'exit 111'
test ! -e q/tmp/001
test ! -e q/tmp/002
test   -f q/queue/001
test   -f q/queue/002
test ! -e q/done/001
test ! -e q/done/002
test ! -e q/fail/001
test ! -e q/fail/002
